{
  "expo": {
    "name": "tracker-v2",
    "slug": "tracker-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "trackerv2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.trackerv2"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.trackerv2",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    }
  }
}
